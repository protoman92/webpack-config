{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": false,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "declaration": true,
    "jsx": "react",
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "target": "es5",
    "sourceMap": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "lib": [
      "es5",
      "es6",
      "dom",
      "es2015.core",
      "es2015.collection",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "esnext.asynciterable"
    ],
    "types": [
      "node",
      "react",
      "jest"
    ],
    "baseUrl": "src",
    "paths": {
      "component/*": [
        "component/*"
      ],
      "css/*": [
        "asset/css/*"
      ],
      "data/*": [
        "data/*"
      ],
      "dependency/*": [
        "dependency/*"
      ],
      "flatten/*": [
        "flatten/*"
      ],
      "images/*": [
        "asset/images/*"
      ],
      "mapper/*": [
        "mapper/*"
      ],
      "redux/*": [
        "redux/*"
      ],
      "route/*": [
        "route/*"
      ],
      "util/*": [
        "util/*"
      ],
      "validator/*": [
        "validator/*"
      ]
    }
  },
  "compileOnSave": true
}